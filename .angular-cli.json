{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "project": {
        "name": "loklak-message-publisher",
        "ejected": false
    },
    "apps": [{
        "root": "src",
        "outDir": "dist",
        "assets": [
            "assets",
            "favicon.ico"
        ],
        "index": "index.html",
        "main": "main.ts",
        "polyfills": "polyfills.ts",
        "test": "test.ts",
        "tsconfig": "tsconfig.app.json",
        "testTsconfig": "tsconfig.spec.json",
        "prefix": "app",
        "styles": [
            "styles.sass",
            "bootstrap3/css/bootstrap.css",
            "assets/css/ct-paper.css",
            "assets/css/demo.css"
        ],
        "scripts": [
            "assets/js/run_prettify.js",
            "assets/js/jquery-1.10.2.js",
            "assets/js/jquery-ui-1.10.4.custom.min.js",
            "bootstrap3/js/bootstrap.js",
            "assets/js/ct-paper-checkbox.js",
            "assets/js/ct-paper-radio.js",
            "assets/js/bootstrap-select.js",
            "assets/js/bootstrap-datepicker.js",
            "assets/js/ct-paper.js"
        ],
        "environmentSource": "environments/environment.ts",
        "environments": {
            "dev": "environments/environment.ts",
            "prod": "environments/environment.prod.ts"
        }
    }],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "lint": [{
            "project": "src/tsconfig.app.json"
        },
        {
            "project": "src/tsconfig.spec.json"
        },
        {
            "project": "e2e/tsconfig.e2e.json"
        }
    ],
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "css",
        "component": {}
    }
}